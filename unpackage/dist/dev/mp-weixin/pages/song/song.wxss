/* 水平间距 */
.song_box {
  position: relative;
  background-repeat: no-repeat !important;
}
.song_box .main {
  position: relative;
  z-index: 999;
}
.song_box .main .pic {
  padding-top: 240rpx;
}
.song_box .main .pic .needle {
  position: absolute;
  top: 0;
  left: 50%;
  -webkit-transform: translateX(-40rpx);
          transform: translateX(-40rpx);
  height: 400rpx;
  width: 240rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  z-index: 1;
}
.song_box .main .pic .vinyl {
  position: relative;
  margin: 0 auto;
  height: 620rpx;
  width: 620rpx;
  overflow: hidden;
}
.song_box .main .pic .vinyl .vinyl_img {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.song_box .main .pic .vinyl .play {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 110rpx;
  height: 110rpx;
  margin-top: -55rpx;
  margin-left: -55rpx;
  background-color: #00000070;
  text-align: center;
  border-radius: 50%;
  line-height: 110rpx;
}
.song_box .main .pic .vinyl image {
  position: absolute;
  top: 50%;
  left: 50%;
  margin: -210rpx 0 0 -210rpx;
  width: 420rpx;
  height: 420rpx;
  border-radius: 50%;
  -webkit-animation: rotate 30s linear infinite;
          animation: rotate 30s linear infinite;
}
@-webkit-keyframes rotate {
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@keyframes rotate {
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
.song_box .main .lyric {
  height: 240rpx;
  text-align: center;
  overflow: hidden;
  margin: 40rpx 0;
}
.song_box .main .lyric .lyric_centent {
  position: relative;
  transition: all 0.5s;
}
.song_box .main .lyric .lyric_centent .txt {
  display: flex;
  justify-content: center;
  align-content: center;
  align-items: center;
  height: 80rpx;
  color: #cccccc50;
  font-size: 30rpx;
}
.song_box .main .lyric .lyric_centent .active {
  color: #fff;
}
.song_box .main .share {
  width: 360rpx;
  height: 80rpx;
  margin: 50rpx auto;
  margin-bottom: 100rpx;
  border: 2rpx solid rgba(255, 255, 255, 0.7);
  background-color: transparent;
  line-height: 80rpx;
  color: #fff;
  font-size: 32rpx;
  border-radius: 50rpx;
}
.song_box .main .main_sum {
  padding: 0 34rpx;
}

