/* 水平间距 */
.simi_container {
  margin-bottom: 40rpx;
}
.simi_container .simi_title {
  display: flex;
  height: 100rpx;
  align-items: center;
  justify-content: space-between;
}
.simi_container .simi_title text {
  font-size: 36rpx;
  color: #fff;
}
.simi_container .simi_title .allPlay {
  margin: 0;
  background-color: transparent;
  border: 2rpx solid #cccccc70;
  border-radius: 50rpx;
  font-size: 26rpx;
  color: #fff;
}
.simi_container .simi_title .allPlay .allPlay_icon {
  margin-right: 5rpx;
}
.simi_container .simi_list .simi_list_item {
  display: flex;
  align-items: center;
  margin: 20rpx 0;
}
.simi_container .simi_list .simi_list_item .cover_img {
  width: 92rpx;
  height: 92rpx;
  overflow: hidden;
  border-radius: 12rpx;
}
.simi_container .simi_list .simi_list_item .cover_img image {
  width: 100%;
  height: 100%;
}
.simi_container .simi_list .simi_list_item .info {
  flex: 1;
  padding-left: 10rpx;
}
.simi_container .simi_list .simi_list_item .info .info_name {
  font-size: 32rpx;
  color: #fff;
  margin-bottom: 12rpx;
}
.simi_container .simi_list .simi_list_item .info .info_artists {
  display: flex;
  align-items: center;
  font-size: 22rpx;
  color: #eee;
}
.simi_container .simi_list .simi_list_item .info .info_artists .sq {
  width: 34rpx;
  height: 24rpx;
  vertical-align: middle;
  margin-right: 5rpx;
}

